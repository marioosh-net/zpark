    	<version>1.0.1</version>
    </dependency>
  </dependencies>
  <build>
    <finalName>mv1</finalName>
    <plugins>
    	<plugin>
    		<artifactId>maven-compiler-plugin</artifactId>
    		<version>2.0.2</version>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>    		
    	</plugin>
    </plugins>
  </build>
